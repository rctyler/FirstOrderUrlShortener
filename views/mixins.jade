mixin bootswatch(theme)
    link(href="//maxcdn.bootstrapcdn.com/bootswatch/3.3.0/" + theme + "/bootstrap.min.css", rel="stylesheet")

mixin nav_item(href, active)
    li(class=active): a( class='mynavbar' href=href )
        block

//- Panel mixin
mixin panel(id, heading, style)
    - var style = (typeof style === 'undefined') ? "default" : style
    div( id=id, class=["panel", "panel-" + style] ng-cloak ng-show="longUrl")
        .panel-heading= heading
        .panel-body
            block